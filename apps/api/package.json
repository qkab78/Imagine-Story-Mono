{
  "name": "@imagine-story/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "db:generate": "npx kysely-codegen --out-file=types/db.ts",
    "db:seed:users": "node ace seed:users",
    "db:seed:stories": "node ace seed:stories",
    "db:migrate": "node ace kysely:migrate",
    "db:rollback": "node ace kysely:rollback",
    "queue:listen": "node ace queue:listen"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/core/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./app/providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#types/*": "./types/*.js",
    "#users/*": "./app/users/*.js",
    "#auth/*": "./app/auth/*.js",
    "#stories/*": "./app/stories/*.js",
    "#payments/*": "./app/payments/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#views/*": "./resources/views/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^1.4.1",
    "@faker-js/faker": "^9.9.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.31",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.4",
    "eslint": "^9.28.0",
    "hot-hook": "^0.4.0",
    "kysely-codegen": "^0.18.5",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/mail": "^9.2.2",
    "@kitajs/html": "^4.2.9",
    "@kitajs/ts-html-plugin": "^4.1.1",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@leonardo-ai/sdk": "^4.20.0",
    "@rlanz/bull-queue": "^3.1.0",
    "@vinejs/vine": "^3.0.1",
    "axios": "^1.9.0",
    "ioredis": "^5.6.1",
    "kysely": "^0.28.2",
    "luxon": "^3.6.1",
    "openai": "^5.1.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  },
  "prettier": "@adonisjs/prettier-config"
}
